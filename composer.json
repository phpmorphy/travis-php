{
    "name": "phpmorphy/travis-php",
    "description": "Travis CI - setup for PHP",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Demo\\": "src/"
        }
    },
    "require": {
        "php-64bit": ">=5.3"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4"
    },
    "scripts": {
        "test": "phpunit",
        "coverage": "phpunit --coverage-html coverage",
    }
}
