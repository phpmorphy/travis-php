{
    "name": "phpmorphy/travis-php",
    "description": "Travis CI - setup for PHP",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Demo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Demo\\Tests\\": "tests/"
        }
    },
    "require": {
        "php-64bit": ">=5.3"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4"
    },
    "config": {
        "disable-tls": true,
        "sort-packages": true,
        "secure-http": false
    },
    "scripts": {
        "test": "phpunit"
    }
}
