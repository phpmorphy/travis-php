{
    "name": "phpmorphy/travis-php",
    "description": "Travis CI - setup for PHP",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Demo\\": "src/"
        }
    },
    "require": {
        "php-64bit": ">=5.3",
        "ext-hash": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4",
        "php-coveralls/php-coveralls": ">=1"
    },
    "suggest": {
        "ext-sodium": "*"
    },
    "scripts": {
        "test": "phpunit",
        "coverage": "phpunit --coverage-html coverage"
    }
}
